{
    "name": "vscode-txl",
    "displayName": "TXL Syntax",
    "description": "Language support for TXL grammar and rule files.",
    "version": "0.0.1",
    "publisher": "potatop",
    "repository": {
        "type": "git",
        "url": "https://github.com/potatop/vscode-txl.git"
    },
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "txl",
            "aliases": ["TXL", "txl"],
            "extensions": [".txl",".grm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "txl",
            "scopeName": "source.txl",
            "path": "./syntaxes/txl.tmLanguage.json"
        }]
    }
}